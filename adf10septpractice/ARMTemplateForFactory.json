		{
			"name": "[concat(parameters('factoryName'), '/dataflow1')]",
			"type": "Microsoft.DataFactory/factories/dataflows",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "MappingDataFlow",
				"typeProperties": {
					"sources": [
						{
							"dataset": {
								"referenceName": "sql_input",
								"type": "DatasetReference"
							},
							"name": "sqlSource"
						}
					],
					"sinks": [
						{
							"dataset": {
								"referenceName": "output_csv",
								"type": "DatasetReference"
							},
							"name": "Sink"
						}
					],
					"transformations": [
						{
							"name": "salarygreaterthan51000"
						}
					],
					"scriptLines": [
						"source(output(",
						"          EmployeeID as integer,",
						"          FirstName as string,",
						"          LastName as string,",
						"          Email as string,",
						"          Phone as string,",
						"          Address as string,",
						"          Department as string,",
						"          Salary as decimal(10,2),",
						"          HireDate as date,",
						"          IsActive as boolean",
						"     ),",
						"     allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     enableNativeCdc: true,",
						"     netChanges: true,",
						"     skipInitialLoad: false,",
						"     isolationLevel: 'READ_UNCOMMITTED',",
						"     format: 'table') ~> sqlSource",
						"sqlSource filter(greaterOrEqual(Salary, 50000)) ~> salarygreaterthan51000",
						"salarygreaterthan51000 sink(allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     umask: 0222,",
						"     preCommands: [],",
						"     postCommands: [],",
						"     skipDuplicateMapInputs: true,",
						"     skipDuplicateMapOutputs: true) ~> Sink"
					]
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/sql_input')]",
				"[concat(variables('factoryId'), '/datasets/output_csv')]"
			]
		}
