{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "sql_input",
						"type": "DatasetReference"
					},
					"name": "sqlSource"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "output_csv",
						"type": "DatasetReference"
					},
					"name": "Sink"
				}
			],
			"transformations": [
				{
					"name": "salarygreaterthan51000"
				}
			],
			"scriptLines": [
				"source(output(",
				"          EmployeeID as integer,",
				"          FirstName as string,",
				"          LastName as string,",
				"          Email as string,",
				"          Phone as string,",
				"          Address as string,",
				"          Department as string,",
				"          Salary as decimal(10,2),",
				"          HireDate as date,",
				"          IsActive as boolean",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     inputs:['@DepartmentName' -> '\\'HR\\''],",
				"     procedureName: 'GetEmployeesByDepartment',",
				"     schemaName: 'dbo',",
				"     resultSet: true,",
				"     format: 'procedure') ~> sqlSource",
				"sqlSource filter(greaterOrEqual(Salary, 51000)) ~> salarygreaterthan51000",
				"salarygreaterthan51000 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> Sink"
			]
		}
	}
}